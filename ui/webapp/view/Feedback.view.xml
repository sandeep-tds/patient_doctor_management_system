<mvc:View 
height="100%" 
controllerName="com.todo.demo.ui.controller.Feedback" 
xmlns:html="http://www.w3.org/1999/xhtml" 
xmlns:mvc="sap.ui.core.mvc"
xmlns:l="sap.ui.layout"
xmlns:f="sap.ui.layout.form"
xmlns:core="sap.ui.core"
displayBlock="true" 
xmlns="sap.m"
>
	<App id="app">
		<pages>
			<Page id="feedback" title=" ">
				<content>
					
				<m:Toolbar>
					<m:ToolbarSpacer/>
					<m:Image src="img/hte.png" width="4%"></m:Image>
				</m:Toolbar>
					
					<HBox justifyContent="Center">
							<Title id="Title1" text="{i18n>HTEC}" textAlign="Center" level="H2" titleStyle="H2"/>
					</HBox>
					<HBox justifyContent="Center">
							<Title id="Title2" text="{i18n>feed1}" textAlign="Center" level="H4" titleStyle="H4"/>
					</HBox>
					<HBox justifyContent="Center">
							<Title id="Title3" text="{i18n>feed2}" textAlign="Center" level="H4" titleStyle="H4"/>
					</HBox>
					<HBox justifyContent="Center">
						<Button icon="sap-icon://activity-individual"
								press="onPress"
						>
						</Button>
						<Button icon="sap-icon://activity-individual"
								press="onPress"
						>
						</Button>
						<Button icon="sap-icon://activity-individual"
								press="onPress"
						>
						</Button>
						<Button icon="sap-icon://activity-individual"
								press="onPress"
						>
						</Button>
						<Button icon="sap-icon://activity-individual"
								press="onPress"
						>
						</Button>
					</HBox>
					<VBox class="sapUiSmallMargin colorBackground" justifyContent="Center" alignItems="Center">
						<f:Form id="FormToolbar" editable="true" ariaLabelledBy="Title1" class="sapUiLargeMargin colorBackgroundForm">
							<f:toolbar>
								<Toolbar id="TB1">
									<!-- TODO Center-->
									<m:Title id="Title4" text="{i18n>Exercice}" textAlign="Center" level="H4" titleStyle="H4"/>
									<ToolbarSpacer/>
								</Toolbar>
							</f:toolbar>
							<f:layout>
								<f:ResponsiveGridLayout/>
							</f:layout>
							<f:formContainers >
								<f:FormContainer ariaLabelledBy="Title2">
									<f:formElements>
										<f:FormElement>
											<f:fields>
												<Input placeholder="{i18n>Freq}" value="{freq}" id="freq"/>
											</f:fields>
										</f:FormElement>
										<!--Put correct type-->
										<f:FormElement>
											<f:fields>
												<Input placeholder="{i18n>Dur}" value="{dur}" id="dur"/>
											</f:fields>
										</f:FormElement>
									
									</f:formElements>
								</f:FormContainer>
							</f:formContainers>
						</f:Form>
					</VBox>
					
					
					<VBox  alignItems="Center">
					
							 	<Title id="Title8" text="{i18n>Effort}" textAlign="Center" level="H4" titleStyle="H4"/>
								<ProgressIndicator
								
								class="sapUiSmallMarginBottom"
								percentValue="30"
								displayValue="30%"
								showValue="true"
								state="None" />
								
  					</VBox>
  					<VBox  alignItems="Center">
					
							 	<Title id="Title9" text="{i18n>Douleur}" textAlign="Center" level="H4" titleStyle="H4"/>
								<ProgressIndicator
								
								class="sapUiSmallMarginBottom"
								percentValue="30"
								displayValue="30%"
								showValue="true"
								state="None" />
								
  					</VBox>
					
					<VBox class="sapUiSmallMargin colorBackground" justifyContent="Center" alignItems="Center">
					
						<f:Form id="FormToolbar2" editable="true" ariaLabelledBy="Title1" class="sapUiLargeMargin colorBackgroundForm">
							<f:toolbar>
								<Toolbar id="TB2">
									<!-- TODO Center-->
									<Title id="Title5" text="{i18n>Commentaires}" textAlign="Center" level="H4" titleStyle="H3"/>
									<Title id="Title6" text="{i18n>Commentaires2}" textAlign="Center" level="H4" titleStyle="H6"/>
									<ToolbarSpacer/>
								</Toolbar>
							</f:toolbar>
							<f:layout>
								<f:ResponsiveGridLayout/>
							</f:layout>
							<f:formContainers >
								<f:FormContainer ariaLabelledBy="Title3">
									<f:formElements>
										<f:FormElement>
											<f:fields>
												<Input placeholder="{i18n>Commentaires}" value="{com}" id="com"/>
											</f:fields>
										</f:FormElement>
									</f:formElements>
								</f:FormContainer>
							</f:formContainers>
						</f:Form>

					
					
					</VBox>
					
					
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>
